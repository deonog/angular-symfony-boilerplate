{
  "name": "angular-symfony-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --poll=2000 --disable-host-check",
    "build": "ng build --base-href /admin --deploy-url /",
    "watch": "encore dev --watch",
    "dev": "npm run watch & npm run start",
    "sass-compile": "encore production --progress",
    "sass-watch": "encore dev --watch",
    "test": "ng test",
    "profile": "ng build --source-map --named-chunks && npx source-map-explorer dist/**/*.js --only-mapped --no-border-checks",
    "package": "mkdir -p ./dist && cp -r ./dist/angular-symfony-boilerplate/browser/* ./dist/ || echo 'Package step: some files not found, but continuing'",
    "buildpack": "npm run sass-compile && npm run build && npm run package || echo 'Build/package completed with warnings'"
  },
  "dependencies": {
    "@angular/animations": "^18.2.5",
    "@angular/cdk": "^18.2.5",
    "@angular/common": "^18.2.5",
    "@angular/compiler": "^18.2.5",
    "@angular/core": "^18.2.5",
    "@angular/forms": "^18.2.5",
    "@angular/material": "^18.2.5",
    "@angular/platform-browser": "^18.2.5",
    "@angular/platform-browser-dynamic": "^18.2.5",
    "@angular/router": "^18.2.5",
    "@ckeditor/ckeditor5-angular": "^7.0.1",
    "@ckeditor/ckeditor5-build-classic": "^41.3.1",
    "class-transformer": "^0.5.1",
    "html2canvas": "^1.4.1",
    "jquery": "^3.7.1",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.8.0",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.19",
    "@angular/cli": "^18.2.19",
    "@angular/compiler-cli": "^18.2.5",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.0",
    "@symfony/webpack-encore": "^4.0.0",
    "@types/event-source-polyfill": "^1.0.5",
    "@types/jasmine": "~5.1.0",
    "@types/jquery": "^3.5.32",
    "autoprefixer": "^10.4.21",
    "core-js": "^3.23.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.4",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.77.8",
    "sass-loader": "^14.2.1",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.4.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-notifier": "^1.15.0"
  }
}
